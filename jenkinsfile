pipeline{
  agent any
  
  stages{
    stage('Build'){
      steps{
        echo 'Building.....'
      }
    }
  
  
    stages{
    stage('Test'){
      steps{
        echo 'Testing.....'
      }
    }
     
    stages{
    stage('Deploy'){
      steps{
        echo 'Deploying.....'
      }
    }
     
    stages{
    stage('Release'){
      steps{
        echo 'Deploy.....'
      }
    }
    
  }
 
}
